<%-  include('./base/header.ejs')  %>

<div class="container">
    <div class="row">
        <h1 class="text-center mt-4 mb-4  p-3">Student List</h1>
        <table class="table text-center table-dark table-hover  table-bordered">
            <thead >
                <tr>
                    <th>Sr.</th>
                    <th>Roll No </th>
                    <th>First Name </th>
                    <th>Last Name </th>
                    <th>Father Name </th>
                    <th>Aadhar Number   </th>
                    <th>Mobile nUmber </th>
                </tr>
            </thead>
            <% let index = 1; %>
            <% students.forEach((student)=>{  %>
                <tr >
                    <td><%= index %> </td>
                    <td> <%= student.rollNo %> </td>
                    <td> <%= student.firstName %></td>
                    <td> <%= student.lastName %></td>
                    <td> <%= student.fatherName %></td>
                    <td> <%= student.adharCardNo %></td>
                    <td> <%= student.mobileNo %></td>
                </tr>
            <% index++; }) %>
        </table>
    </div>
</div>

<%-  include('./base/footer.ejs')  %>
     